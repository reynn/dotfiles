{
  "snippets.ultisnips.enable": true,
  "snippets.ultisnips.directories": [
    "snippets/ultisnips"
  ],
  "snippets.priority": 99,
  "codeLens.enable": false,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "go"
      ],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  }
}
